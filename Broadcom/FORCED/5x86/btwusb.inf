;
; Special sections for Broadcom Smart Dongle
;
[BRSMARTUSB.NTx86.hw]
AddReg=BRSMARTUSB.hw.reg

[BRSMARTUSB.NTx86]
AddReg=BTWUSB.reg
CopyFiles=BTWUSB.CopyFiles.NT

[BRSMARTUSB.NTx86.Services]
Addservice = BTWUSB, 0x00000002, BTWUSB.AddService

[BRSMARTUSB.hw.reg]
HKR,,"SmartDongleType",0x10001,2
HKR,,"RemoteWakeupDisabled",0x10001,0
HKR,,"IdleSuspendEnabled",0x10001,1

;
; Special sections for Broadcom Smart Dongle (No Suspend)
;
[BRSMARTNOSUSPEND.NTx86.hw]
AddReg=BRSMARTNOSUSPEND.hw.reg

[BRSMARTNOSUSPEND.NTx86]
AddReg=BTWUSB.reg
CopyFiles=BTWUSB.CopyFiles.NT

[BRSMARTNOSUSPEND.NTx86.Services]
Addservice = BTWUSB, 0x00000002, BTWUSB.AddService

[BRSMARTNOSUSPEND.hw.reg]
HKR,,"SmartDongleType",0x10001,2
HKR,,"RemoteWakeupDisabled",0x10001,0
HKR,,"IdleSuspendEnabled",0x10001,0


;
; Special sections for the Dell and other 
; "selectively suspendable" dongles
;
[SUSPENDUSB.NTx86.hw]
AddReg=SUSPENDUSB.hw.reg

[SUSPENDUSB.NTx86]
AddReg=BTWUSB.reg
CopyFiles=BTWUSB.CopyFiles.NT

[SUSPENDUSB.NTx86.Services]
Addservice = BTWUSB, 0x00000002, BTWUSB.AddService

[SUSPENDUSB.hw.reg]
HKR,,"RemoteWakeupDisabled",0x10001,0
HKR,,"IdleSuspendEnabled",0x10001,1


;
; Special sections for the 20702 et al
; RAM patch downloadable dongles
;

[RAMUSBClean.hw.reg]
HKR,,"RAMPatchFileName"

[RAMUSB065A.NTx86.hw]
AddReg=RAMUSB065A.hw.reg

[RAMUSB065A.NTx86]
AddReg=BTWUSB.reg
CopyFiles=RAMUSB065A.CopyFiles.NT

[RAMUSB065A.NTx86.Services]
Addservice = BTWUSB, 0x00000002, BTWUSB.AddService
AddService=btwsecfl,,BTWSECFL_Service_Inst, BTWSECFL_EventLog_Inst

[RAMUSB065A.hw.reg]
HKR,,"RemoteWakeupDisabled",0x10001,0
HKR,,"IdleSuspendEnabled",0x10001,1
HKR,,"RAMPatchFileName",0x10000, "BCM20702A1_001.002.014.0889.0927.hex"
HKR, ,LowerFilters, 0x00010000, "btwsecfl"

[RAMUSB065A.CopyFiles.NT]
btwusb.sys
btwsecfl.sys
BCM20702A1_001.002.014.0889.0927.hex

