;
; Widcomm BTW Virtual HID Minidriver Installation Script
; for the 64-bit Windows
;
; Copyright (c) 2004, Broadcom Corp.
;
[version]
signature="$Windows NT$"
Class=HIDClass
ClassGUID={745a17a0-74d3-11d0-b6fe-00a0c90f57da}
Provider=%V_WIDCOMM%
CatalogFile=btwhidnt.cat
DriverVer=11/17/2009,5.6.0.3000

[DestinationDirs]
CopyFiles.amd64 = 12

[SourceDisksNames]
1 = %Source_Description%,,,

[SourceDisksFiles]
btwhid.sys = 1

[Manufacturer] 
%V_WIDCOMM%=WIDCOMM, NTamd64

[WIDCOMM.NTamd64]
%WC_KEYB_DESCRIPTION%=WC_KEYB, {95C7A0A0-3094-11D7-A202-00508B9D7D5A}\HID_KEYBOARD
%WC_MOUSE_DESCRIPTION%=WC_MOUSE, {95C7A0A0-3094-11D7-A202-00508B9D7D5A}\HID_MOUSE
%WC_AVCONTROL_DESCRIPTION%=WC_AVCONTROL, {95C7A0A0-3094-11D7-A202-00508B9D7D5A}\HIDAVCONTROL

;
; Virtual keyboard device
;
[WC_KEYB.NTamd64.hw]
AddReg=WC_KEYB.hw.reg

[WC_KEYB.hw.reg]
HKR,,"Usage",0x10001,%HID_USAGE_GENERIC_KEYBOARD%

[WC_KEYB.NTamd64]
CopyFiles = CopyFiles.amd64
Include=input.inf
Copyfiles=HID_Inst.CopyFiles.NT

[WC_KEYB.NTamd64.Services]
AddService = btwhid,0x00000002,BTWHID_Serv_Instx86, 

;
; Virtual mouse device
;
[WC_MOUSE.NTamd64.hw]
AddReg=WC_MOUSE.hw.reg

[WC_MOUSE.hw.reg]
HKR,,"Usage",0x10001,%HID_USAGE_GENERIC_MOUSE%

[WC_MOUSE.NTamd64]
CopyFiles = CopyFiles.amd64
Include=input.inf
Copyfiles=HID_Inst.CopyFiles.NT

[WC_MOUSE.NTamd64.Services]
AddService = btwhid,0x00000002,BTWHID_Serv_Instx86, 

;
; Virtual AV control device
;
[WC_AVCONTROL.NTamd64.hw]
AddReg=WC_AVCONTROL.hw.reg

[WC_AVCONTROL.hw.reg]
HKR,,"Usage",0x10001,%HID_USAGE_GENERIC_AVCONTROL%

[WC_AVCONTROL.NTamd64]
CopyFiles = CopyFiles.amd64
Include=input.inf
Copyfiles=HID_Inst.CopyFiles.NT

[WC_AVCONTROL.NTamd64.Services]
AddService = btwhid,0x00000002,BTWHID_Serv_Instx86, 

[BTWHID_Serv_Instx86] 
ServiceType = %SERVICE_KERNEL_DRIVER% 
StartType = %SERVICE_DEMAND_START% 
ErrorControl = %SERVICE_ERROR_IGNORE% 
ServiceBinary = %12%\btwhid.sys 

[CopyFiles.amd64] 
btwhid.sys

