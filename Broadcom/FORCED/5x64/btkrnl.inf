[Version]
Signature="$WINDOWS NT$"
Class=BTW
ClassGUID={95C7A0A0-3094-11D7-A202-00508B9D7D5A}
Provider=%V_WIDCOMM%
CatalogFile=btw.cat
DriverVer=02/13/2013,5.6.0.8800

[SourceDisksNames]
1 = %Source_Description%,,,

[SourceDisksFiles]
btkrnl.sys  = 1
btw_ci.dll = 1

[ClassInstall32]
AddReg=BTW.NT.AddReg
CopyFiles=BTW.NT.CopyFiles

[BTW.NT.AddReg]
HKR,,,0,%BTWClassName%
HKR,,Icon,,100
HKR,,Installer32,,"btw_ci.dll,BTWClassInstaller"

[BTW.NT.CopyFiles]
btw_ci.dll

[DestinationDirs]
DefaultDestDir   = 12
BTW.NT.CopyFiles = 11
Drivers_Dir      = 12

[Manufacturer]
%V_WIDCOMM%=WIDCOMM, NTamd64

[WIDCOMM.NTamd64]
%Btkrnl.DeviceDesc%=Btkrnl_Device, {95C7A0A0-3094-11D7-A202-00508B9D7D5A}\BTKRNL

[Btkrnl_Device.NTamd64]
CopyFiles=Drivers_Dir

[Drivers_Dir]
btkrnl.sys

[Btkrnl_Device.NTamd64.Services]
AddService = btkrnl,%SPSVCINST_ASSOCSERVICE%, btkrnl_Service_Inst

[btkrnl_Service_Inst]
DisplayName    = %btkrnl.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START 
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\btkrnl.sys
LoadOrderGroup = Extended Base


