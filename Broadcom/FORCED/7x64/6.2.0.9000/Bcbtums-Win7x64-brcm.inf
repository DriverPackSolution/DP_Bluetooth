[BCBTBLPMInstall.NT]
Include=Bth.inf
Needs=BthUsb.NT
CopyFiles=BTUSBFLT.CopyList

[BCBTBLPMInstall.NT.HW]
AddReg=BCBTBLPMInstall.NT.HW.AddReg
Needs=BthUsb.NT.HW

[BCBTBLPMInstall.NT.Services]
AddReg=BCBTUsb.NT.Services.AddReg
Addservice = BTHUSB, 0x00000002, BthUsb_Service_Inst, BthUsb_EventLog_Inst
Addservice = BTHPORT, , BthPort_Service_Inst, BthPort_EventLog_Inst
AddService=btusbflt,,BTUSBFLT_Service_Inst

[BCBTBLPMInstall.NT.HW.AddReg]
HKR,,%RemoteWakeEnabled%,0x00010001,1
HKR,,%DeviceRemoteWakeSupported%,0x00010001,1
HKR,,LowerFilters, 0x00010000, "btusbflt"

[BTUSBFLT_Service_Inst]
DisplayName=%BTUSBFLT.SvcDesc%
ServiceType=1
StartType=3
ErrorControl=1
ServiceBinary=%10%\system32\drivers\btusbflt.sys
LoadOrderGroup = PNP Filter


