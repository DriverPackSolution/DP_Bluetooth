[BCBTUsbDriverInstallXP.NT]
Include=Bth.inf
Needs=BthUsb.NT

[BCBTUsbDriverInstallXP.NT.HW]
AddReg=BCBTUsbDriverInstallXP.NT.HW.AddReg
Needs=BthUsb.NT.HW

